startshape RA_TEST []

shape SQ_TEST
{
	SQUARE [ b 1 sat 1 ]

	loop i=4 [ x 0.25 ]
	{
		loop j=4 [ y 0.25 ]
		{
			SQUARE [  x (-0.5+(1/8)) y (-0.5+(1/8)) s 0.25 a -0.9 ]
		}
	}
}

shape RA_TEST
{
	RA [ sat 1  b 1]
	RA [ s 0.95 ]

	RA_TEST [ s 0.7 ] 

	//SQUARE   [ s 0.5 x 0.25 y -0.25 ]

	//RA   [ s 0.5 x 0.25 y 0.25 ]
	//RA   [ s 0.5 x -0.25 y -0.25 ]
}

//vertex points for unit right angle
ra_x1 = -1 * ( sqrt ( 2 ) / 4 ) / tan ( 45 )
ra_y1 = -1 - ra_x1

path RA
{
	MOVETO ( ra_x1, ra_y1 )

	LINETO (   ra_x1, ra_y1+1 )
	LINETO ( ra_x1+1, ra_y1+1 )

	CLOSEPOLY ()

	STROKE () []
	FILL  () []
}
